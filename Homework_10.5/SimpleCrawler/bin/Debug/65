</span><span style="color: rgba(0, 128, 0, 1)">//</span><span style="color: rgba(0, 128, 0, 1)">删除</span>
<span style="color: rgba(0, 0, 255, 1)">function</span><span style="color: rgba(0, 0, 0, 1)"> deleteCargo() {
    $(</span>"#del").click(<span style="color: rgba(0, 0, 255, 1)">function</span><span style="color: rgba(0, 0, 0, 1)"> () {
        </span><span style="color: rgba(0, 0, 255, 1)">var</span> rows = $("#dgCargo").datagrid("getSelections"<span style="color: rgba(0, 0, 0, 1)">);
        </span><span style="color: rgba(0, 0, 255, 1)">if</span> (rows.length &gt; 0<span style="color: rgba(0, 0, 0, 1)">) {
            $.messager.confirm(</span>"提示", "确定要删除吗?", <span style="color: rgba(0, 0, 255, 1)">function</span><span style="color: rgba(0, 0, 0, 1)"> (res) {
               </span><span style="color: rgba(0, 0, 255, 1)">if</span><span style="color: rgba(0, 0, 0, 1)"> (res) {
                    </span><span style="color: rgba(0, 0, 255, 1)">var</span> codes = []; <span style="color: rgba(0, 128, 0, 1)">//</span><span style="color: rgba(0, 128, 0, 1)">重要不是{}</span>
                    <span style="color: rgba(0, 0, 255, 1)">for</span> (<span style="color: rgba(0, 0, 255, 1)">var</span> i = 0; i &lt; rows.length; i++<span style="color: rgba(0, 0, 0, 1)">) {
                        codes.push(rows[i].Id);
                    }
                    $.post(</span>"/Cargo/Delete", { "ids": codes.join(',') }, <span style="color: rgba(0, 0, 255, 1)">function</span><span style="color: rgba(0, 0, 0, 1)"> (data) {
                        </span><span style="color: rgba(0, 0, 255, 1)">if</span> (data == "OK"<span style="color: rgba(0, 0, 0, 1)">) {
                            $.messager.alert(</span>"提示", "删除成功！"<span style="color: rgba(0, 0, 0, 1)">);
                            $(</span>"#dgCargo").datagrid("clearChecked"<span style="color: rgba(0, 0, 0, 1)">);
                            $(</span>"#dgCargo").datagrid("clearSelections"<span style="color: rgba(0, 0, 0, 1)">);
                            $(</span>"#dgCargo").datagrid("load"<span style="color: rgba(0, 0, 0, 1)">, {});
                        }                       
                        </span><span style="color: rgba(0, 0, 255, 1)">else</span> <span style="color: rgba(0, 0, 255, 1)">if</span> (data == "NO"<span style="color: rgba(0, 0, 0, 1)">) {
                            $.messager.alert(</span>"提示", "删除失败！"<span style="color: rgba(0, 0, 0, 1)">);
                            </span><span style="color: rgba(0, 0, 255, 1)">return</span><span style="color: rgba(0, 0, 0, 1)">;
                        }
                    });
                }
            });
        }
    })
}

