        toolbar: [
            { text: </span>'添加明细', iconCls: 'icon-add', handler: <span style="color: rgba(0, 0, 255, 1)">function</span><span style="color: rgba(0, 0, 0, 1)"> () { ShowCargoInfo(); } },
            { text: </span>'添加库位', iconCls: 'icon-edit', handler: <span style="color: rgba(0, 0, 255, 1)">function</span><span style="color: rgba(0, 0, 0, 1)"> () { SubGridAddRow(); } },
