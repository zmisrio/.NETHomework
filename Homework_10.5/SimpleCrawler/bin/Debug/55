        [DontWrapResult]
        </span><span style="color: rgba(0, 0, 255, 1)">public</span> <span style="color: rgba(0, 0, 255, 1)">string</span> ImportCargo(<span style="color: rgba(0, 0, 255, 1)">string</span> ids,<span style="color: rgba(0, 0, 255, 1)">string</span><span style="color: rgba(0, 0, 0, 1)"> No)
