        </span><span style="color: rgba(0, 0, 255, 1)">protected</span> <span style="color: rgba(0, 0, 255, 1)">override</span> IQueryable&lt;InStockOrder&gt;<span style="color: rgba(0, 0, 0, 1)"> CreateFilteredQuery(PagedInStockResultRequestDto input)
        {

            </span><span style="color: rgba(0, 0, 255, 1)">var</span> qry= <span style="color: rgba(0, 0, 255, 1)">base</span><span style="color: rgba(0, 0, 0, 1)">.CreateFilteredQuery(input)
                .Where(t</span>=&gt;t.OwnerName.Contains(input.OwnerName==<span style="color: rgba(0, 0, 255, 1)">null</span>?<span style="color: rgba(0, 0, 255, 1)">string</span><span style="color: rgba(0, 0, 0, 1)">.Empty:input.OwnerName))
                 .Where(t </span>=&gt; t.No.Contains(input.No == <span style="color: rgba(0, 0, 255, 1)">null</span> ? <span style="color: rgba(0, 0, 255, 1)">string</span><span style="color: rgba(0, 0, 0, 1)">.Empty : input.No))

                .Where(t </span>=&gt; t.CreationTime&gt;<span style="color: rgba(0, 0, 0, 1)">input.BeginTime)
                .Where(t </span>=&gt; t.CreationTime&lt;<span style="color: rgba(0, 0, 0, 1)">input.EndTime);
            </span><span style="color: rgba(0, 0, 255, 1)">return</span><span style="color: rgba(0, 0, 0, 1)"> qry;

        }

