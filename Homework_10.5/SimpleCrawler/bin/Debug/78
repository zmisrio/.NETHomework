                    {
                        list.AddRange(listUpdated.ToArray());

                    }
                }

                </span><span style="color: rgba(0, 0, 255, 1)">if</span> (iso == <span style="color: rgba(0, 0, 255, 1)">null</span><span style="color: rgba(0, 0, 0, 1)">)
